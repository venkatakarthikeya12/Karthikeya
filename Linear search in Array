#include<stdio.h>
int main() {
 int n,x;
    int m=0;
    printf("enter a size of array :");
    scanf("%d",&n);
    int a[n];
    printf(" enter a array values:");
    for(int i=0;i<n;i++) {
        scanf("%d",&a[i]); }
    printf("enter a search elment number:");
    scanf("%d",&x);
    for(int i=0;i<n;i++) {
        if(a[i]==x)  {
          m=1;
          break; } } 
    if(m==1)   {
        printf(" element %d is present in array :");  }
    else {
        printf(" element %d is not present in array :"); }
    return 0;
}

INPUT : enter a size of array : 5
        enter a array values : 1 5 6 8 9

OUTPUT : enter a search element number : 6
         element '6' is present in array
OUTPUT : enter a search element number : 3
         element '3' is not present in array
