#include<stdio.h>
int main() {
int n,m=0;
printf("enter a size of an array :");
scanf(" %d ",&n);
int a[n];
printf("enter a elements of array : ");
for(int i=0;i<n;i++) {
scanf(" %d ",&a[i]); }
int x;
printf("enter a number ");
scanf("%d",&x); 
int max,min,mid;
max = n-1;
min = 0;
while(min <= max) {
if(min + max / 2 ) { 
if(x == a[mid]) {
m == 1;
printf(" %d is present in index :",x,mid);
if(x<a[mid]) {
max = mid +1; }
else {
min = mid-1; } } } }
if(m ! = 1) {
printf(" %d is not exist in index",x); }
return 0;
}

INPUT : enter a size of an array : 5
        enter a elements in a array : 5 6 3 4 9
OUTPUT : enter a element : 6
         6 is present in index

OUTPUT : enetr a element 7
        7 is not exist in index
         

